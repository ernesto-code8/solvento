{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
      "plugin:prettier/recommended",
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
  ],
  "plugins": ["prettier","@typescript-eslint"],
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "project": "./tsconfig.json"
  },
  "rules": {
    "prettier/prettier": "warn"
  }
}